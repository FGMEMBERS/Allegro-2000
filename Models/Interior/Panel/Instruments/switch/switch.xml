<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>switch.ac</path>

  <animation>
    <object-name>switch</object-name>
    <object-name>info</object-name>
    <object-name>lampOFF</object-name>
    <object-name>lampON</object-name>
    
  </animation>

  <animation>
    <type>material</type>
    <object-name>switch</object-name>
    <object-name>info</object-name>
    <condition>
      <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lampON</object-name>
    <emission>
      <red>   1 </red>
      <green> 2 </green>
      <blue>  1 </blue>
    </emission>
  </animation>
  
</PropertyList>
