<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>switch2.ac</path>
  
  <animation>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
    <condition>
      <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>
  
</PropertyList>
