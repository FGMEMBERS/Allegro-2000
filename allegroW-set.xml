<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="allegro-base.xml">

  <sim>
    
    <description>Allegro 2000 (Wheels)</description>
    <author>Emmanuel BARANGER (3D/FDM)</author>
    <flight-model archive="y">yasim</flight-model>
    <aero archive="y">allegroW-yasim</aero>
    
    <rating>
      <FDM type="int">     2 </FDM>
      <systems type="int"> 1 </systems>
